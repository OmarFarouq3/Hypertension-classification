    "precision = [0.839, 0.747, 0.998, 1.0, 0.833]\n",
    "recall = [0.917, 0.841, 0.997, 1.0, 0.867]\n",
    "f1 = [0.876, 0.791, 0.997, 1.0, 0.849]\n",
